msgstr "Συγχρονισμός τώρα"

#. type=template
msgctxt "template"
msgid "Story text"
msgstr "Κείμενο Ιστορίας"

#. type=template
msgctxt "template"
msgid "First reading"
msgstr "Πρώτη ανάγνωση"

#. type=template
msgctxt "template"
msgid "Second reading"
msgstr "Δεύτερη ανάγνωση"

#. type=template
